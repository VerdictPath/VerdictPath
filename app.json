{
  "expo": {
    "name": "Verdict Path",
    "slug": "verdict-path",
    "version": "1.0.1",
    "owner": "verdictpath",
    "scheme": "verdictpath",
    "icon": "./attached_assets/Nautical Pirate Logo with Foggy Sea Background_1762830868803.png",
    "extra": {
      "eas": {
        "projectId": "71e06a4e-c10f-4b46-92d3-4f091343570c"
      }
    },
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./attached_assets/Nautical Pirate Logo with Foggy Sea Background_1762830868803.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C5F7C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.verdictpath.app",
      "associatedDomains": ["applinks:verdictpath.com"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./attached_assets/Nautical Pirate Logo with Foggy Sea Background_1762830868803.png",
        "backgroundColor": "#2C5F7C"
      },
      "package": "com.verdictpath.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "verdictpath"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "experiments": {
      "baseUrl": "/"
    }
  }
}
