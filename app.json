{
  "expo": {
    "name": "Verdict Path",
    "slug": "verdict-path",
    "version": "1.0.4",
    "owner": "verdictpath",
    "scheme": "verdictpath",
    "icon": "./attached_assets/verdict_path_logo.png",
    "extra": {
      "eas": {
        "projectId": "4dccfd60-99f8-4c20-ad4d-3675503fbdb5"
      }
    },
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./attached_assets/verdict_path_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C5F7C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.verdictpath.app",
      "associatedDomains": [
        "applinks:verdictpath.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./attached_assets/verdict_path_logo.png",
        "backgroundColor": "#2C5F7C"
      },
      "package": "com.verdictpath.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "verdictpath"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./attached_assets/verdict_path_logo.png",
      "name": "Verdict Path",
      "shortName": "VerdictPath",
      "description": "Legal case management and education platform for civil litigation",
      "themeColor": "#2C5F7C",
      "backgroundColor": "#000000",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "lang": "en"
    },
    "experiments": {
      "baseUrl": "/"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ]
  }
}
