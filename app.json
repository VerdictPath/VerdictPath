{
  "expo": {
    "name": "Verdict Path",
    "slug": "verdict-path",
    "version": "1.0.1",
    "owner": "verdictpath",
    "scheme": "verdictpath",
    "extra": {
      "eas": {
        "projectId": "71e06a4e-c10f-4b46-92d3-4f091343570c"
      }
    },
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#3498db"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.verdictpath.app",
      "associatedDomains": ["applinks:verdictpath.com"]
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3498db"
      },
      "package": "com.verdictpath.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "verdictpath"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "build": {
        "pwa": false
      }
    }
  }
}
